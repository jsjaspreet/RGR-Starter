{
  "name": "rag-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:server": "webpack --config config/webpack/server.js",
    "build:client": "webpack --config config/webpack/client.js",
    "build:client-prod": "cross-env NODE_ENV=production webpack -p --config config/webpack/client.js",
    "build:server-prod": "cross-env NODE_ENV=production webpack -p --config config/webpack/server.js",
    "build:prod": "npm run clean && npm run build:server-prod && npm run build:client-prod",
    "build": "npm run clean && npm run build:server && npm run build:client",
    "clean": "rimraf build",
    "update-relay": "npm run update-schema && npm run relay",
    "dev:client": "webpack-dev-server --config config/webpack/client.js",
    "update-schema": "babel-node ./scripts/updateSchema.js",
    "dev": "npm run clean && npm run build:server && npm run dev:server & npm run dev:client",
    "relay": "relay-compiler --src ./src --schema ./schema.graphql",
    "dev:server": "nodemon --watch src/server --exec \"npm run start:server\"",
    "start": "node build/server.js",
    "start:server": "cross-env babel-node src/server/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-relay": "^1.0.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",
    "html-webpack-plugin": "^2.28.0",
    "nodemon": "^1.11.0",
    "relay-compiler": "^1.0.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-md5-hash": "^0.0.5",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "autobind-decorator": "^2.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "express": "^4.15.2",
    "express-graphql": "^0.6.4",
    "graphql": "^0.9.6",
    "graphql-relay": "^0.5.2",
    "humps": "^2.0.0",
    "join-monster": "^1.2.4-beta.1",
    "js-cookie": "^2.1.4",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "material-ui": "^0.18.5",
    "moment": "^2.18.1",
    "pg": "^6.2.3",
    "pg-escape": "^0.2.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-fontawesome": "^1.6.1",
    "react-hot-loader": "next",
    "react-relay": "^1.0.0",
    "react-router-dom": "^4.1.1",
    "react-stack-grid": "^0.2.2",
    "slug": "^0.9.1",
    "uuid": "^3.1.0"
  },
  "babel": {
    "presets": [
      "env",
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react",
      "stage-1"
    ],
    "plugins": [
      "relay",
      "react-hot-loader/babel",
      "transform-class-properties",
      "syntax-dynamic-import",
      "transform-decorators-legacy"
    ]
  }
}
